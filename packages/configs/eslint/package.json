{
  "name": "@relatos/config-eslint",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "ESLint configurations for the Relatos de Barrios monorepo",
  "main": "./src/base.js",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/base.js"
    },
    "./next": {
      "types": "./src/index.d.ts",
      "default": "./src/next.js"
    },
    "./react": {
      "types": "./src/index.d.ts",
      "default": "./src/react.js"
    }
  },
  "files": [
    "*.ts",
    "src/**/*.ts",
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "devDependencies": {
    "@relatos/config-typescript": "workspace:*",
    "eslint": "catalog:",
    "@eslint/js": "^9.29.0",
    "@next/eslint-plugin-next": "^15.3.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-turbo": "^2.5.0",
    "globals": "^16.2.0",
    "typescript-eslint": "^8.34.0"
  }
}
