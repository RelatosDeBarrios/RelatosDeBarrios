{
  "name": "@relatos/content",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "description": "Shared content management package with business data, UI texts, team information, and brand content for the Relatos de Barrios monorepo",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./core": {
      "types": "./src/core/index.ts",
      "import": "./src/core/index.ts",
      "require": "./src/core/index.ts"
    },
    "./apps": {
      "types": "./src/apps/index.ts",
      "import": "./src/apps/index.ts",
      "require": "./src/apps/index.ts"
    },
    "./ui": {
      "types": "./src/ui/index.ts",
      "import": "./src/ui/index.ts",
      "require": "./src/ui/index.ts"
    },
    "./seo": {
      "types": "./src/seo/index.ts",
      "import": "./src/seo/index.ts",
      "require": "./src/seo/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "import": "./src/types/index.ts",
      "require": "./src/types/index.ts"
    }
  },
  "files": [
    "src/**/*"
  ],
  "devDependencies": {
    "@relatos/config-typescript": "workspace:*",
    "@relatos/config-eslint": "workspace:*",
    "eslint": "catalog:"
  }
}
